<h1>Susbcribe to a Feed</h1>

<%= form_tag(new_feed_path, method: :get) do %>
    <%= label_tag(:search, "Search by URL") %>
    <%= text_field_tag(:search, params[:search]) %>
    <%= submit_tag ("Search") %>
<% end %>

<h3>Results</h3>

<%= form_tag new_feed_path, method: :post do %>
    <%= @feed.title %>
    <%= submit_tag "Subscribe to Feed" %>
<% end %>

  

