<h1>Susbcribe to a Feed</h1>

<%= form_tag(feeds_path, method: :get) do %>
    <%= label_tag(:search, "Search by URL") %>
    <%= text_field_tag(:search, params[:search]) %>
    <%= submit_tag ("Search") %>
<% end %>

<h3>Results</h3>

    <%= link_to @feed.title, feed_path(@feed) %><br>
    <%= @feed.description %>

  

